<Window x:Class="W.TrayIcon.WPF.Sample.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:W.TrayIcon.WPF.Sample" 
        xmlns:w="http://schemas.wssids.com/wpf/w/trayicon"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <w:TrayIconControl IsShow="True" Icon="/Icon128.png" ToolTip="{Binding Test}"/>
    </Grid>
</Window>
